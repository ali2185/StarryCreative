<html>

<head>
    <meta charset= "utf-8"/>
    <title> Starry Summer Members Only </title>
    <link href=https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css rel="stylesheet">
    <link rel="stylesheet" href="public/css/styles.css">

</head>

<header id="header background" style="width:100%;
	height:300px;
	background: url(/public/images/voicetone.jpg)">
		<div class="logo">
		</div>
	</header>
<br>
<body>
    <div class="container">

<% if (commentsData) { %>


    <script>
        var receivedData = <%- JSON.stringify(commentsData) %>;
        console.log(receivedData);
    </script>
    <br>
    <h2>Contact Starry - Messages Received</h2>
    <br>

    <img class="img5" src="/public/images/messages.jpeg" alt="Woman writes a letter">

    <table>
        <tr>
            <th>Name</th>
            <th>Message</th>
            <th>Email</th>
            <th>Date Message Sent</th>
        </tr>
        <% for (var comment of commentsData) { %>
            <tr>
                <td><%= comment.name %></td>
                <td><%= comment.message %></td>
                <td><%= comment.email %></td>
                <td><%= comment.date %></td>
            </tr>
        <% } %>

        <!-- <p>
            <%- JSON.stringify(commentsData) %>
        </p> -->


<% } else {%>

    <h2>No messages yet</h2>

<% } %>

</div>
</body>

</html>